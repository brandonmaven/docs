{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "Maven",
  "logo": {
    "light": "/logo/maven.png",
    "dark": "/logo/maven.png"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "fonts": {
    "heading": {
      "family": "Inter",
      "weight": 600
    },
    "body": {
      "family": "Inter",
      "weight": 400
    }
  },
  "background": {
    "decoration": "grid"
  },
  "styling": {
    "eyebrows": "breadcrumbs",
    "codeblocks": "dark"
  },
  "icons": {
    "library": "lucide"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://prod.trymaven.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://prod.trymaven.com"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "icon": "book-open",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "authentication",
              "integrations/webhooks",
              "voice-settings"
            ]
          },
          {
            "group": "Voice Providers",
            "pages": [
              "integrations/vapi-integration",
              "integrations/retell-integration"
            ]
          },
          {
            "group": "Payment Gateways",
            "pages": [
              "integrations/stripe-connect",
              "integrations/authorizenet-setup",
              "integrations/braintree-setup"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "square-terminal",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/overview"
            ]
          },
          {
            "group": "Auth",
            "openapi": "openapi.json",
            "pages": [
              "GET /v1/auth/check"
            ]
          },
          {
            "group": "Sessions",
            "openapi": "openapi.json",
            "pages": [
              "POST /v1/sessions",
              "GET /v1/sessions",
              "GET /v1/sessions/{session_id}",
              "POST /v1/sessions/{session_id}/cancel"
            ]
          },
          {
            "group": "Apps",
            "openapi": "openapi.json",
            "pages": [
              "GET /v1/projects"
            ]
          },
          {
            "group": "Health",
            "openapi": "openapi.json",
            "pages": [
              "GET /health"
            ]
          }
        ]
      }
    ]
  },
  "api": {
    "mdx": {
      "server": "https://pciapi.trymaven.com",
      "auth": {
        "method": "bearer"
      }
    },
    "playground": {
      "display": "interactive"
    }
  },
  "footer": {
    "socials": {
      "website": "https://trymaven.com",
      "github": "https://github.com/mavenagi"
    }
  },
  "feedback": {
    "thumbsRating": true
  }
}
