{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Maven",
  "colors": {
    "primary": "#8B5CF6",
    "light": "#A78BFA",
    "dark": "#7C3AED"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "get-started/introduction",
              "get-started/quickstart",
              "get-started/how-it-works",
              "get-started/authentication"
            ]
          },
          {
            "group": "Payment Processors",
            "pages": [
              "get-started/stripe",
              "get-started/braintree"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integration/vapi",
              "integration/retell",
              "integration/twilio",
              "integration/livekit"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Reference",
            "pages": [
              "api-reference/overview",
              "api-reference/errors"
            ]
          },
          {
            "group": "Sessions",
            "pages": [
              "api-reference/sessions/create",
              "api-reference/sessions/get",
              "api-reference/sessions/get-by-caller",
              "api-reference/sessions/cancel"
            ]
          },
          {
            "group": "Projects",
            "pages": [
              "api-reference/projects/list"
            ]
          },
          {
            "group": "Auth",
            "pages": [
              "api-reference/auth/check"
            ]
          }
        ]
      }
    ],
    "global": {}
  },
  "redirects": [
    { "source": "/api-reference/auth", "destination": "/api-reference/auth/check" },
    { "source": "/api-reference/projects", "destination": "/api-reference/projects/list" }
  ],
  "logo": {
    "light": "/logo/maven.png",
    "dark": "/logo/maven.png",
    "href": "https://trymaven.com"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:founders@trymaven.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.trymaven.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mavenpayments",
      "linkedin": "https://linkedin.com/company/trymaven"
    }
  },
  "metadata": {
    "og:site_name": "Maven Docs",
    "og:description": "Collect payments over the phone with AI voice agents",
    "twitter:site": "@trymaven"
  },
  "seo": {
    "indexHiddenPages": false
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  }
}
